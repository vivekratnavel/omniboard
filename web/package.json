    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-preset-react-app": "^8.0.0-next.74",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dom-helpers": "^3.3.1",
    "dotenv": "4.0.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "file-saver": "^2.0.0-rc.3",
    "fixed-data-table-2": "^0.8.21",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "ify-loader": "^1.1.0",
    "jest": "^24.1.0",
    "jsondiffpatch": "^0.3.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "ms": "^2.1.1",
    "object-assign": "4.1.1",
    "plotly.js": "^1.33.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "pretty-ms": "^5.0.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "rc-slider": "^8.6.4",
    "rc-util": "^4.6.0",
    "react": "^16.8.6",
    "react-accessible-accordion": "^2.4.4",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-multiselect": "^2.4.1",
    "react-bootstrap-xeditable": "^0.2.10",
    "react-dev-utils": "^4.2.1",
    "react-diff-viewer": "^2.0.1",
    "react-dom": "16.8.6",
    "react-json-view": "^1.16.0",
    "react-list": "^0.8.11",
    "react-localstorage": "^0.3.1",
    "react-mixin": "2",
    "react-numeric-input": "^2.2.3",
    "react-plotly.js": "^1.3.0",
    "react-render-html": "^0.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.0.0",
    "react-switch": "^4.1.0",
    "react-syntax-highlighter": "^9.0.0",
    "react-table": "^6.7.6",
    "react-toastify": "^4.2.0",
    "reactn": "^1.0.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "weak": "^1.0.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "debug": "node --inspect scripts/start.js",
    "build": "node scripts/build.js",
    "test": "TZ='America/New_York' node --expose-gc --max-old-space-size=2048 scripts/test.js --env=jsdom --runInBand --logHeapUsage --detectOpenHandles --forceExit",
    "lint": "xo src/*",
    "lint:fix": "xo --fix src/*",
    "coverage": "yarn run test --coverage",
    "test:debug": "TZ='America/New_York' node --inspect-brk scripts/test.js --runInBand --env=jsdom",
    "report-coverage": "npm run coverage -- --coverageReporters=text-lcov | coveralls"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/jestSetup.js",
      "jest-canvas-mock",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/momentMock.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-proposal-class-properties"
    ],
    "sourceMap": "inline"
  },
  "xo": {
    "space": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "16.8.6"
      }
    },
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "import/no-unassigned-import": 0,
      "camelcase": 0,
      "react/state-in-constructor": 0,
      "react/boolean-prop-naming": 0,
      "promise/prefer-await-to-then": 0,
      "react/require-default-props": 0,
      "react/default-props-match-prop-types": 0,
      "unicorn/prevent-abbreviations": 0,
      "react/no-array-index-key": 0,
      "no-return-assign": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "no-unused-vars": [
        2,
        {
          "argsIgnorePattern": "^_\\w*",
          "varsIgnorePattern": "^_\\w*"
        }
      ],
      "eslint-comments/disable-enable-pair": [
        2,
        {
          "allowWholeFile": true
        }
      ]
    },
    "globals": [
      "shallow",
      "mount",
      "tick"
    ],
    "env": [
      "node",
      "es6",
      "mocha",
      "jest",
      "browser"
    ],
    "extends": [
      "xo-react/space"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true,
      "jest": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "16.8.6"
      }
    },
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "import/no-unassigned-import": 0,
      "camelcase": 0,
      "react/state-in-constructor": 0,
      "react/boolean-prop-naming": 0,
      "react/default-props-match-prop-types": 0,
      "promise/prefer-await-to-then": 0,
      "react/require-default-props": 0,
      "unicorn/prevent-abbreviations": 0,
      "react/no-array-index-key": 0,
      "no-return-assign": 0,
      "unicorn/filename-case": [
        1,
        {
          "case": "camelCase"
        }
      ],
      "no-unused-vars": [
        2,
        {
          "argsIgnorePattern": "^_\\w*",
          "varsIgnorePattern": "^_\\w*"
        }
      ],
      "eslint-comments/disable-enable-pair": [
        2,
        {
          "allowWholeFile": true
        }
      ]
    },
    "extends": [
      "xo/esnext",
      "xo-react/space",
      "./node_modules/xo/config/plugins.js"
    ],
    "globals": {
      "shallow": true,
      "mount": true,
      "tick": true
    }
  },
  "proxy": "http://localhost:9000/",
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.18",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.8.10",
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "jest-canvas-mock": "^2.0.0-alpha.3",
    "jest-fetch-mock": "^2.1.1",
    "jest-localstorage-mock": "^2.4.0",
    "jest-mock-axios": "^2.1.11",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0-beta.21",
    "sass-loader": "^6.0.6",
    "xo": "^0.24.0"
  }
}
