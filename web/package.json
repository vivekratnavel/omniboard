{
  "name": "omniboard-web",
  "version": "0.0.1",
  "description": "Web-based dashboard for Sacred",
  "author": "Vivek Ratnavel Subramanian <vivekratnavel.s@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vivekratnavel/omniboard.git"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dom-helpers": "^3.3.1",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "file-saver": "^2.0.0-rc.3",
    "fixed-data-table-2": "^0.8.9",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "ify-loader": "^1.1.0",
    "jest": "20.0.4",
    "jszip": "^3.1.5",
    "ms": "^2.1.1",
    "object-assign": "4.1.1",
    "plotly.js": "^1.33.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-accessible-accordion": "^2.4.4",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-multiselect": "^2.4.1",
    "react-bootstrap-xeditable": "^0.2.10",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-json-view": "^1.16.0",
    "react-localstorage": "^0.3.1",
    "react-mixin": "2",
    "react-plotly.js": "^1.3.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.0.0",
    "react-syntax-highlighter": "^9.0.0",
    "react-table": "^6.7.6",
    "react-toastify": "^4.2.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/*",
    "coverage": "npm test -- --coverage",
    "test:debug": "node --inspect-brk scripts/test.js --runInBand --env=jsdom",
    "report-coverage": "npm run coverage -- --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/jestSetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:9000/",
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.10",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "jest-mock-axios": "^2.1.11",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0-beta.21",
    "sass-loader": "^6.0.6"
  }
}
